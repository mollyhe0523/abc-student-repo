{
  "name":"Click",
  "version":"1.0",
  "description":"Click",
  "manifest_version":2,
  "permissions":[
    "storage"
  ],
  "content_scripts":[
    {
      "matches": ["<all_urls>"],
      "js":["contentscript.js"]
    }
  ],
  "browser_action":{
    "default_icon": "tent.png",
    "default_popup": "popup/popup.html"
  },
  "background": {
      "scripts": ["background.js"],
      "persistent": false
    }

}
