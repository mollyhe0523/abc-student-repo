{
  "name":"Replacer",
  "version":"1.0",
  "description":"Replace words on the web with your words",
  "manifest_version":2,
  "permissions":[
    "activeTab"
  ],
  "content_scripts":[
    {
      "matches": ["<all_urls>"],
      "js":["contentscript.js"]
    }
  ],
  "browser_action":{
    "default_icon": "tent.png",
    "default_popup": "popup/popup.html"
  }
}
